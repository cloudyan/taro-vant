  "main": "./cjs/index.js",
  "unpkg": "./umd/antd-mobile.js",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "typings": "./es/index.d.ts",
  "files": [
    "./lib"
  ],
  "scripts": {
    "build": "gulp",
    "build-doc": "dumi build && echo '!.*' > dist/.surgeignore",
    "eslint": "eslint ./src",
    "eslint:fix": "eslint --fix ./src",
    "prettier": "prettier --check ./src",
    "prettier:fix": "prettier --write ./src",
    "stylelint": "stylelint \"**/*.{css,less,scss}\"",
    "stylelint:fix": "stylelint --fix \"**/*.{css,less,scss}\"",
    "biome:ci": "biome ci ./src",
    "biome:check": "biome format ./src",
    "biome:format": "biome format --write ./src",
    "oxlint": "oxlint -c ./.eslintrc.json ./src",
    "compat-check": "eslint --no-eslintrc --ext .js main.js",
    "postinstall": "husky install",
    "lint": "eslint . && stylelint \"**/*.{css,less,scss}\"",
    "lint:fix": "prettier --write . && eslint --fix . && stylelint --fix \"**/*.{css,less,scss}\"",
    "package-diff": "antd-tools run package-diff --path=./lib",
    "prepare": "husky install",
    "pub": "npm run package-diff && npm publish ./lib",
    "pub:alpha": "npm publish ./lib --tag alpha",
    "pub:dev": "npm publish ./lib --tag dev",
    "size-limit": "size-limit",
    "size-limit:ci": "size-limit --json",
    "dev": "dumi dev",
    "test": "jest",
    "test-with-coverage": "jest --coverage"
  },
  "resolutions": {
    "@types/react": "18",
    "@types/react-dom": "18"
  },
